{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "max-len": 0,
    "semi": ["error", "never"],
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0
  },
  "globals": {
    "preval": true
  }
}
